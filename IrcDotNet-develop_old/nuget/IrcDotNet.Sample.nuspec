<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>@project@</id>
    <title>IRC.NET Samples</title>
    <version>@build.number@</version>
    <authors>@authors@</authors>
    <owners>@authors@</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/alexreg/ircdotnet/blob/develop/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/alexreg/ircdotnet</projectUrl>
    <summary>
      Sample projects for IRC.NET library (.NET 4.0)
    </summary>
    <description>
      These sample projects demonstrate how the IRC.NET library can be used in various ways.
    </description>
    <releaseNotes>@releaseNotes@</releaseNotes>
    <copyright>Copyright 2015</copyright>
    <tags>@tags@</tags>
    @dependencies@
  </metadata>
  <files>
    <file src="..\..\src\Samples\**" target="content\Samples\" />
    <file src="..\..\README.md" target="content\App_Readme\IrcDotNet.readme.md" />
    <file src="..\..\AUTHORS.md" target="content\App_Readme\IrcDotNet.authors.md" />
    <file src="..\..\LICENSE.md" target="content\App_Readme\IrcDotNet.license.md" />
  </files>
</package>

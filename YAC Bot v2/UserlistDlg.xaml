<Window x:Class="YAC_Bot_v2.UserlistDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YAC_Bot_v2"
        mc:Ignorable="d"
        Title="UserlistDlg" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="dataGrid" Margin="0,0,0,0" BorderThickness="0">
            <DataGrid.Columns>
                <DataGridCheckBoxColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Usernames" HeaderStringFormat="" MinWidth="80" Width="80"/>
                <DataGridCheckBoxColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Counter" HeaderStringFormat=""/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
